FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottom
    {
        type            uniformFixedValue;
        uniformValue    constant (0 0 0);
    }

   
    top
    {
        type            uniformFixedValue;
        uniformValue    constant (0 0 0);
    }

    side
    {
        type            uniformFixedValue;
        uniformValue    constant (0 0 0);
    }

    defaultFaces
    {
        type            empty;
    }

    inlet
    {
        type            uniformFixedValue;
        uniformValue     table
        (
            (0.000  (0 0 0))
            (0.01   (0 0 0))
            (0.15   (0 0 -2.5))
            (0.25   (0 0 -5))
            (0.35   (0 0 -10))
            (0.45   (0 0 -20))
            (0.55   (0 0 -50))
        );
    }


}

// ************************************************************************* //
